     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Execution Time: 0:00:19.376143\n",
      "prompt_token_count: 476673\n",
      "candidates_token_count: 566\n",
      "total_token_count: 477239\n",
      "cached_content_token_count: 476644\n",
