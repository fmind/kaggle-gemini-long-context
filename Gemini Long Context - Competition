     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Execution Time: 0:00:08.229369\n",
      "prompt_token_count: 235\n",
      "candidates_token_count: 998\n",
      "total_token_count: 1233\n",
      "\n"
     ]
